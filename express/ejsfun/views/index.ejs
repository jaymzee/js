<div class="jumbotron">
  <h3 class='display-3'>Variable</h3>
  <p class="lead"><%= tagline %></p>
  <ul>
    <% mascots.forEach(mascot => { %>
      <li>
        <strong><%= mascot.name %></strong>
        representing <%= mascot.organization %>,
        born <%= mascot.birth_year %>
      </li>
    <% }); %>
  </ul>
</div>
